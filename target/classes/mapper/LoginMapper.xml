<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.storefamily.mapper.LoginMapper">

	<select id="getLogin" parameterType="String" resultType="Member">
		select * from member where id=#{id} and pwd=#{pwd}
	</select>

	<select id="getStore" parameterType="String" resultType="Member">
		select * from store where code=#{code} and id=#{id}
	</select>
</mapper>